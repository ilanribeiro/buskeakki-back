{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "quotes": [
        "error",
        "single",
        { "avoidEscape": true, "allowTemplateLiterals": false }
      ]
    }
}
